{
  "name": "reset-program-tests",
  "version": "0.1.0",
  "description": "TypeScript tests for Reset Program",
  "scripts": {
    "test": "ANCHOR_PROVIDER_URL=http://127.0.0.1:8899 ANCHOR_WALLET=$HOME/.config/solana/id.json ts-node tests/run-all-tests.ts",
    "build": "tsc",
    "build:sdk": "cd reset-sdk && npm run build",
    "build:sdk:watch": "cd reset-sdk && npm run build:watch",
    "clean:sdk": "cd reset-sdk && npm run clean",
    "lint": "eslint tests/**/*.ts",
    "format": "prettier --write **/*.{ts,tsx,js,jsx,css,scss,json,md}"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "prettier": "3.5.3",
    "ts-mocha": "11.1.0",
    "typescript": "5.8.3",
    "vitest": "3.1.2"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.31.1",
    "@solana-program/token-2022": "^0.4.2",
    "@solana/compat": "^2.3.0",
    "@solana/instructions": "^2.3.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/spl-token-metadata": "^0.1.6",
    "@solana/web3.js": "^1.91.6",
    "bn.js": "^5.2.1",
    "decimal": "^0.0.2",
    "decimal.js": "^10.5.0",
    "tweetnacl": "^1.0.3"
  }
}
